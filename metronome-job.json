{
  "id": "jobs-test-sleep",
  "schedules": [
    {
      "id": "every-minute",
      "enabled": true,
      "cron": "* * * * *",
      "concurrencyPolicy": "ALLOW"
    }
  ],
  "run": {
    "cmd": "/bin/sh -c 'for i in {1..30}; do echo \"Run $i...\"; sleep 1; done'",
    "cpus": 0.1,
    "mem": 32,
    "disk": 128,
    "docker": {
      "image": "alpine"
    }
  }
}
